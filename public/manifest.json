{
  "manifest_version": 3,
  "name": "DSAGENIE",
  "version": "1.1.0",
  "description": "Instant solutions and video explanations for LeetCode DSA problems",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "action": {
    "default_popup": "index.html",
    "default_title": "DSAGENIE"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://leetcode.com/*"],
      "js": ["content.js"],
      "css": ["content.css"]
    }
  ]
}